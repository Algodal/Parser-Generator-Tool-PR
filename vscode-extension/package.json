{
  "name": "algodal-parser",
  "displayName": "algodal-parser",
  "description": "Algodal Parser Generator Language",
  "version": "2.0.2",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "algodal-parser",
